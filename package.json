{
  "name": "seditor",
  "version": "1.0.0",
  "description": "A neat interface for interacting with a fully realized docker development environment",
  "keywords": [],
  "author": "Kurtis Rainbolt-Greene (@krainboltgreene) <kurtis@rainbolt-greene.online>",
  "license": "ISC",
  "homepage": "https://github.com/krainboltgreene/seditor#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krainboltgreene/seditor.git"
  },
  "bugs": {
    "url": "https://github.com/krainboltgreene/seditor/issues"
  },
  "build": {
    "appId": "seditor",
    "productName": "seditor",
    "directories": {
      "buildResources": "./tmp/",
      "output": "./package",
      "app": "./tmp/"
    },
    "publish": [
      {
        "provider": "github"
      }
    ]
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "prestart": "npm run build",
    "start": "electron './tmp/server/index.js'",
    "package": "electron-builder build --dir",
    "release": "electron-builder build",
    "build:server": "gulp build:server",
    "build:client": "gulp build:client",
    "build": "gulp build:all",
    "watch:lint": "chokidar './source/' --ignore '/node_modules/' --command 'npm run lint --silent'",
    "watch:test": "chokidar './source/' --ignore '/node_modules/' --command 'npm run test --silent'",
    "watch:build": "gulp watch:all",
    "watch": "npm-run-all --parallel watch:build watch:lint watch:test",
    "test": "tap './source/**/test.js'",
    "lint": "eslint './source/' --ignore-pattern 'node_modules'"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-module-resolver": "2.7.1",
    "babel-plugin-pipe-operator-curry": "1.0.2",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.0",
    "babelify": "7.3.0",
    "browserify": "14.4.0",
    "chokidar-cli": "1.2.0",
    "electron": "1.7.5",
    "electron-builder": "19.27.7",
    "envify": "4.1.0",
    "eslint": "3.19.0",
    "eslint-plugin-promise": "3.5.0",
    "firacode": "1.205.0",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-changed": "3.1.0",
    "gulp-cli": "1.4.0",
    "gulp-concat": "2.6.1",
    "gulp-environments": "0.1.2",
    "gulp-myth": "1.1.0",
    "gulp-rename": "1.2.2",
    "gulp-size": "2.1.0",
    "npm-run-all": "4.1.1",
    "octicons": "6.0.1",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0"
  },
  "dependencies": {
    "@cycle/dom": "17.4.0",
    "@cycle/run": "3.1.0",
    "@unction/allp": "2.3.0",
    "@unction/always": "2.3.0",
    "@unction/arrayify": "2.4.0",
    "@unction/catchp": "2.3.0",
    "@unction/domevents": "2.3.0",
    "@unction/domeventsmany": "2.3.0",
    "@unction/equals": "1.4.0",
    "@unction/flip": "2.3.0",
    "@unction/ispopulated": "4.3.0",
    "@unction/istype": "2.3.0",
    "@unction/mapvalues": "3.3.0",
    "@unction/mapwithvaluekey": "1.0.3",
    "@unction/mergeallright": "2.3.0",
    "@unction/mergedeepright": "2.3.0",
    "@unction/mergeleft": "3.2.0",
    "@unction/optimisticp": "3.3.0",
    "@unction/pipe": "2.3.0",
    "@unction/recordfrom": "2.3.0",
    "@unction/resolvep": "2.3.0",
    "@unction/splat": "2.3.0",
    "@unction/thenp": "2.3.0",
    "@unction/zip": "3.3.0",
    "bunyan": "1.8.12",
    "dotenv": "4.0.0",
    "electron-context-menu": "0.9.1",
    "escape-html": "1.0.3",
    "fs-extra": "3.0.1",
    "glob": "7.1.2",
    "glob-promise": "3.1.0",
    "highlight.js": "9.12.0",
    "moment": "2.18.1",
    "normalize.css": "7.0.0",
    "ramda": "0.24.1",
    "require-environment-variables": "1.1.2",
    "snabbdom-helpers": "4.0.3",
    "snabbdom-virtualize": "0.7.0",
    "store": "2.0.12",
    "url-parse": "1.1.9",
    "xstream": "10.9.0"
  }
}
